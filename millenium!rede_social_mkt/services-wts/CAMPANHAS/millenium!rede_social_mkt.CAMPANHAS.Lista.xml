<?xml version="1.0"?>
<root>
  <OBJECT NAME="CAMPANHAS">
    <METHOD NAME="Lista" DESCRIPTION="Lista Campanhas" VERSION="30" INTFTYPE="5" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="REDES_SOCIAIS" FORMAT="R" FLAGS="1" STYLE="6" PROJECTION="0" ORDER="1" LOOKUP="REDES_SOCIAIS.LISTA" LOOKUPKEY="REDE_SOCIAL" LOOKUPDISPLAY="DESCRICAO" FIELDLABEL="Rede Social" NESTEDNAME="WTSSYSTEM.INTERNALTYPES.INTEGERARRAY" CTAB="0"/>
        <PARAM NAME="NUMERO_CUPOM" SIZE="50" PROJECTION="0" FIELDLABEL="Numero Cupom" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="CAMPANHA" FORMAT="+" SIZE="8" FLAGS="1024" PROJECTION="0" FIELDLABEL="Campanha" CTAB="0"/>
        <FIELD NAME="NUMERO_CUPOM" SIZE="50" FLAGS="17" PROJECTION="0" ORDER="3" FIELDLABEL="N&#250;mero Cupom" CTAB="0"/>
        <FIELD NAME="DESCRICAO" SIZE="100" FLAGS="17" PROJECTION="0" ORDER="4" FIELDLABEL="Descri&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="REDE_SOCIAL" SIZE="50" FLAGS="17" PROJECTION="0" ORDER="5" FIELDLABEL="Rede Social" CTAB="0"/>
        <FIELD NAME="DATA_INICIAL" FORMAT="D" SIZE="10" FLAGS="17" PROJECTION="0" ORDER="6" FIELDLABEL="Data Inical" CTAB="0"/>
        <FIELD NAME="DATA_FINAL" FORMAT="D" SIZE="10" FLAGS="17" PROJECTION="0" ORDER="7" FIELDLABEL="Data Final" CTAB="0"/>
        <FIELD NAME="RSM_PERCENTUAL_DESCONTO" FORMAT="N" SIZE="8" FLAGS="19" PROJECTION="0" ORDER="8" FIELDLABEL="Percentual Desconto" CTAB="0"/>
        <FIELD NAME="VALOR_MAXIMO_COMPRA" FORMAT="M" SIZE="17" FLAGS="17" DECIMALS="4" PROJECTION="0" ORDER="9" FIELDLABEL="Valor M&#225;ximo Compra" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT C.CAMPANHA,
       C.DESCRICAO,
       R.DESCRICAO AS REDE_SOCIAL,
       C.NUMERO_CUPOM,
       C.DATA_INICIAL,
       C.DATA_FINAL,
       C.PERCENTUAL_DESCONTO As RSM_PERCENTUAL_DESCONTO,
       C.VALOR_MAXIMO_COMPRA
FROM RSM_CAMPANHAS C
INNER JOIN RSM_REDES_SOCIAIS R ON (R.REDE_SOCIAL=C.REDE_SOCIAL)
WHERE [C.REDE_SOCIAL IN #MAKELIST(REDES_SOCIAIS,ITEM)];
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>