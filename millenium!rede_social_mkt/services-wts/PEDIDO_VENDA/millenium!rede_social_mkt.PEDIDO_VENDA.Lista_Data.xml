<?xml version="1.0"?>
<root>
  <OBJECT NAME="PEDIDO_VENDA">
    <METHOD NAME="Lista_Data" DESCRIPTION="" VERSION="20" INTFTYPE="5" TRIGGEROF="PEDIDO_VENDA.LISTA_DATA" TRIGGERTYPE="0" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP NAME="Marketing Rede Social"/>
        </GROUPS>
        <PARAM NAME="REDE_SOCIAL" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="2" LOOKUP="REDES_SOCIAIS.Lista" LOOKUPKEY="REDE_SOCIAL" LOOKUPDISPLAY="DESCRICAO" FIELDLABEL="Rede Social" GROUPNAME="Marketing Rede Social" CTAB="0"/>
        <PARAM NAME="NUMERO_CUPOM" SIZE="50" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Numero Cupom" GROUPNAME="Marketing Rede Social" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="REDE_SOCIAL" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Rede Social" CTAB="0"/>
        <FIELD NAME="NUMERO_CUPOM" SIZE="50" FLAGS="1" PROJECTION="0" ORDER="5" FIELDLABEL="N&#250;mero Cupom" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT TMP.*,
       PV.NUMERO_CUPOM,
       RS.DESCRICAO AS REDE_SOCIAL
FROM #REPLACE(:PARENT_RESULT) TMP
LEFT JOIN RSM_PEDIDO_VENDA PV ON (PV.PEDIDOV = TMP.PEDIDOV)
LEFT JOIN RSM_CAMPANHAS CA ON (CA.CAMPANHA = PV.CAMPANHA)
LEFT JOIN RSM_REDES_SOCIAIS RS ON (RS.REDE_SOCIAL = CA.REDE_SOCIAL)
WHERE [PV.NUMERO_CUPOM=:NUMERO_CUPOM]
      [AND RS.REDE_SOCIAL=:REDE_SOCIAL];
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>